<%- include("./layouts/header"); -%>
<%- include("./layouts/navbar"); -%>

<% if(success[0]||error[0]){ %>
  <main class="container mt-3">
      <%- include("./layouts/flash"); -%>
  </main>
  <% } %> 
  
    <% user.forEach((u)=>{%>
        <% if(u.compny===compny.name){ %> 
                 
            <div class="profile-detail">
                <div class="profile-image">
                    <% if(typeof home !== "undefined"){ %> 
                        <% if(home.googleId) {%> 
                            <img  src="<%=u.logo%>">
                            <% } else{ %>
                              <img  src="/<%=u.logo%>">
                              <% } %> 
                              <% } %> 
                    </div>
                   <div class="profile-name">
                    <h5 class="text_profile"><%=u.name%></h5>
                    <p class="text_profile">Student at NIT Mizoram</p>
                    <p class="text_profile">Bihar,India</p>
                    <p class="text_profile">Age- <%=u.age%></p>
                    <p class="text_profile">Email: <%=u.username%> </p>
                    <p class="text_profile">Currently working in :<a href="/compny/tech/<%=compny._id %> "><%=u.compny %></a> </p>
                    </div>
                    <div class="about-section">
                      <div class="about-inner">
                      <h6>About</h6>
                      <p>First of all thank you for visiting here. Currently I am pursuing B-tech from NIT Mizoram. I am someone who loves to write code and solving problems. Helping student at Chegg India.</p>
                    </div>
                    </div>
                 </div>

   <% } %> 

   <% }) %> 


  <%- include("./layouts/footer"); -%>