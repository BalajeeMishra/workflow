  <div class="profile-detail">
    <div class="profile-image">
      <% if(home.googleId) {%> 
        <img  src="<%=home.logo%>">
        <% } else{ %>
          <img  src="/<%=home.logo%>">
          <% } %> 
        </div>
       <div class="profile-name">
        <h5 class="text_profile"><%=home.name%></h5>
